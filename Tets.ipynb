   "source": [
    "function B4_to_B_eff(B)\n",
    "    hc = 197.33\n",
    "    return B^4 / (hc^3) \n",
    "end\n",
    "\n",
    "function B_eff_to_B4(B_eff)\n",
    "    hc = 197.33\n",
    "    return (B_eff * (hc^3))^(1/4)\n",
    "end"
   ]
